<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="htlgrieskirchen.net.tawimmer.todo_newtry">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_list_alt_24"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_baseline_list_alt_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.ToDo_newTry">
        <activity
            android:name=".DrawerMenuActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ToDo_newTry.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingActivity"
            android:theme="@style/Theme.ToDo_newTry.NoActionBar">

        </activity>
    </application>

</manifest>